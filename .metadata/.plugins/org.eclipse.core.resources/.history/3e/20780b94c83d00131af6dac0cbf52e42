package com.homework.additional;

public class Direction{
	public enum Direction { 
	 	   high { 
	 	        public Direction opposite() { return high; } 
	 	   }, 
	 	   medium { 
	 	        public Direction opposite() { return medium; } 
	 	   },
	 	   lower { 
		        public Direction opposite() { return lower; } 
		   }, 
		   prelower { 
		        public Direction opposite() { return prelower; } 
		   }; 
	 	 
	 	   public abstract Direction opposite(); 
	 	}
}
 