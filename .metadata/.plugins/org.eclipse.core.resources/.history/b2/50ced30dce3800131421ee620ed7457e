
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int method=4;
		switch (method) 
		{ 
			case 0:
				System.out.println("0");
				break; 
			case 1: 
				System.out.println("1");
				break; 
			case 2:
				System.out.println("2");
				break; 
			default:
				System.out.println("Invalid numbre of test");
		}
		
	}

}
